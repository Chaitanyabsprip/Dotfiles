{
  "coc.preferences.enableMessageDialog": true,
  "coc.preferences.formatOnSaveFiletypes": [
    "css",
    "dart",
    "fish",
    "html",
    "javascript",
    "json",
    "python",
    "sh",
    "toml",
    "yaml"
  ],
  "codeLens.enable": true,
  "codeLens.subseparator": ":|",
  "diagnostic.enable": true,
  "diagnostic.errorSign": "E>",
  "diagnostic.hintSign": "h>",
  "diagnostic.infoSign": "I>",
  "diagnostic.virtualText": true,
  "diagnostic.virtualTextCurrentLineOnly": false,
  "diagnostic.virtualTextPrefix": "~",
  "diagnostic.warningSign": "⚠ >",
  "explorer.icon.enableNerdfont": true,
  "explorer.keyMappings.global": {
    "<cr>": ["expandable?", "expand", "open"],
    "v": "open:vsplit"
  },
  "explorer.previewAction.onHover": false,
  "explorer.width": 30,
  "flutter.enabled": true,
  "flutter.lsp.debug": true,
  "flutter.lsp.initialization.onlyAnalyzeProjectsWithOpenFiles": false,
  "flutter.lsp.initialization.closingLabels": true,
  "flutter.lsp.initialization.outline": true,
  "flutter.provider.enableSnippet": true,
  "flutter.runDevToolsAtStartup": true,
  "python.linting.enabled": true,
  "python.linting.pylintEnabled": true,
  "suggest.enablePreview": true,
  "suggest.triggerAfterInsertEnter": true,
  "suggest.completionItemKindLabels": {
    "keyword": "",
    "variable": "",
    "value": "",
    "operator": "",
    "constructor": "",
    "function": "",
    "reference": "渚",
    "constant": "",
    "method": "",
    "struct": "פּ",
    "class": "",
    "interface": "",
    "text": "",
    "enum": "",
    "enumMember": "",
    "module": "",
    "color": "",
    "property": "",
    "field": "料",
    "unit": "",
    "event": "鬒",
    "file": "",
    "folder": "",
    "snippet": "",
    "typeParameter": "",
    "default": ""
  }
}
